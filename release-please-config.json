{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "exclude-paths": ["packages", "website"]
    },
    "packages/core": {
      "component": "@ts-endpoint/core"
    },
    "packages/http-client": {
      "component": "@ts-endpoint/http-client"
    },
    "packages/resource-client": {
      "component": "@ts-endpoint/resource-client"
    },
    "packages/react-admin": {
      "component": "@ts-endpoint/react-admin"
    },
    "packages/tanstack-query": {
      "component": "@ts-endpoint/tanstack-query"
    },
    "packages/express": {
      "component": "@ts-endpoint/express"
    }
  },
  "versioning": "prerelease",
  "bootstrap-sha": "2a0d464cf8a3aae27e4e0b057da04d4988018334",
  "initial-version": "0.1.0",
  "tag-separator": "@",
  "include-v-in-tag": false,
  "group-pull-request-title-pattern": "chore: release ${version}",
  "plugins": ["node-workspace"],
  "changelog-sections": [
    {
      "type": "chore",
      "section": "Miscellaneous Chores",
      "hidden": false
    },
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    }
  ]
}
